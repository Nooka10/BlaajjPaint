<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.HBox?>

<HBox spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.tools.ParamCropController">
   <children>
      <Button fx:id="cancelButton" cancelButton="true" mnemonicParsing="false" onAction="#handleCancel" prefWidth="100.0" text="Annuler">
         <tooltip>
            <Tooltip text="Cliquez pour annuler le rognage du calque sélectionné." wrapText="true" />
         </tooltip></Button>
      <Button fx:id="validateButton" defaultButton="true" mnemonicParsing="false" onAction="#handleValidate" prefWidth="100.0" text="Valider">
         <tooltip>
            <Tooltip text="Cliquez pour valider le rognage du calque sélectionné" wrapText="true" />
         </tooltip></Button>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
