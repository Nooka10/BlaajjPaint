<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>

<HBox spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.tools.ParamCropController">
   <children>
      <Button fx:id="cancelButton" cancelButton="true" mnemonicParsing="false" onAction="#cancel" prefWidth="100.0" text="Annuler" />
      <Button fx:id="validateButton" defaultButton="true" mnemonicParsing="false" onAction="#validate" prefWidth="100.0" text="Valider" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
