<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.rightMenu.LayerController">
	<children>
		<HBox fx:id="layerElem" onMouseClicked="#handleMouseClicked" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
		      AnchorPane.topAnchor="0.0">
			<children>
				<CheckBox fx:id="visibility" mnemonicParsing="false" onAction="#handleVisibilityChange"/>
				<Label fx:id="label"/>
			</children>
		</HBox>
	</children>
</AnchorPane>
